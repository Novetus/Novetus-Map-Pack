			<Item class="Script" referent="RBX3">
				<Properties>
					<bool name="Archivable">true</bool>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">Game</string>
					<ProtectedString name="Source">--CHANGE THESE IF YOU WANT TO BALANCE THE GAME ACCORDING TO HOW LARGE YOUR MAP IS.
local enemycountlimit = 20
local enemyaddmin = 1
local enemyaddmax = 3
local bossbattlewave = 5
--DON&apos;T CHANGE ANYTHING BELOW
game.Lighting.WaveMaxForBoss.Value = bossbattlewave

local hint = Instance.new(&quot;Hint&quot;)
hint.Name = &quot;Hint&quot;
hint.Parent = script.Parent
hint.Text = &quot;5 seconds till first wave!&quot;
wait(5)

while true do
	wait(0.2)

	if game.Lighting.WaveStarted.Value == false then
		game.Lighting.Wave.Value = game.Lighting.Wave.Value + 1
		game.Lighting.WaveForBoss.Value = game.Lighting.WaveForBoss.Value + 1

		if game.Lighting.WaveForBoss.Value &lt; bossbattlewave then
			if game.Lighting.Wave.Value &gt; 1 then
				if game.Lighting.DefaultEnemyCount.Value &lt; enemycountlimit then
					game.Lighting.DefaultEnemyCount.Value = game.Lighting.DefaultEnemyCount.Value + math.random(enemyaddmin,enemyaddmax)
				else
					game.Lighting.DefaultEnemyCount.Value = enemycountlimit
				end
			else
				if game.Lighting.DefaultEnemyCount.Value &lt; enemycountlimit then
					game.Lighting.DefaultEnemyCount.Value = game.Lighting.DefaultEnemyCount.Value + 1
				else
					game.Lighting.DefaultEnemyCount.Value = enemycountlimit
				end
			end
		elseif game.Lighting.WaveForBoss.Value &gt; bossbattlewave then
			if game.Lighting.PreBossDefaultEnemyCount.Value &lt; enemycountlimit then
				game.Lighting.DefaultEnemyCount.Value = game.Lighting.PreBossDefaultEnemyCount.Value + math.random(enemyaddmin,enemyaddmax)
			else
				game.Lighting.PreBossDefaultEnemyCount.Value = enemycountlimit
				game.Lighting.DefaultEnemyCount.Value = game.Lighting.PreBossDefaultEnemyCount.Value
			end
			
			game.Lighting.WaveForBoss.Value = 1
			game.Lighting.PreBossDefaultEnemyCount.Value = 0
		elseif game.Lighting.WaveForBoss.Value == game.Lighting.WaveMaxForBoss.Value then
			game.Lighting.WaveNeedsBoss.Value = true
		end

		if game.Lighting.WaveNeedsBoss.Value == true then
			-- BOSS BATTLE
			game.Lighting.PreBossDefaultEnemyCount.Value = game.Lighting.DefaultEnemyCount.Value
			game.Lighting.DefaultEnemyCount.Value = 1
		end

		game.Lighting.EnemyCount.Value = game.Lighting.DefaultEnemyCount.Value
		game.Lighting.WaveStarted.Value = true

		while game.Lighting.WaveStarted.Value == true do
			wait(0.2)

			hint.Text = &quot;Wave: &quot;..game.Lighting.Wave.Value..&quot; | Enemy Count: &quot;..game.Lighting.EnemyCount.Value..&quot;/&quot;..game.Lighting.DefaultEnemyCount.Value
			
			if game.Lighting.SpawnedEnemyCount.Value ~= game.Lighting.DefaultEnemyCount.Value then
				wait(2)
				local bot = game.Lighting.Killbot:clone()
				if bot:findFirstChild(&quot;Head&quot;) then
					bot.Head.Position = script.Parent.Position
				end
				bot.Parent = game.Workspace
				game.Lighting.SpawnedEnemyCount.Value = game.Lighting.SpawnedEnemyCount.Value + 1
			end
			wait(1)

			if game.Lighting.EnemyCount.Value &lt; 1 then
				game.Lighting.WaveStarted.Value = false
				game.Lighting.WaveNeedsBoss.Value = false
				game.Lighting.SpawnedEnemyCount.Value = 0
				hint.Text = &quot;5 seconds till next wave!&quot;
				wait(5)
			end
		end
	end
end
</ProtectedString>
