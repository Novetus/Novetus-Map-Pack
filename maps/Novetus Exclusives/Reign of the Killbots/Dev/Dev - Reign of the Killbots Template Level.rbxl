	local lookAt = snap( (targetPos - character.Head.Position).unit )
	local cf = CFrame.new(targetPos, targetPos + lookAt)

	Tool.Handle.BuildSound:play()

	buildWall(cf)
	
	if brickNumCur == brickNum then
		Tool.Enabled = true
		brickNumCur = 0
	end
end

